


def transpose(matrix):
    transposed = []
    rows = len(matrix)
    cols = len(matrix[0])

    for j in range(cols):
        transposed_row = [matrix[i][j] for i in range(rows)]
        transposed.append(transposed_row)

    return transposed


# 示例用法
A = [[1, 2, 3],
     [4, 5, 6]]


# 调用自定义函数
B = transpose(A)
B
